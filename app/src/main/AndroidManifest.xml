<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pc.energywatch">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".regis" android:screenOrientation="portrait"/>
        <activity android:name=".forget" android:screenOrientation="portrait" />
        <activity android:name=".Main" android:screenOrientation="portrait"/>
        <activity android:name=".infor" android:screenOrientation="portrait"/>
        <activity android:name=".data" android:screenOrientation="portrait"></activity>
    </application>

</manifest>